{
  "name": "anywize",
  "version": "0.1.0",
  "engines": {
    "node": ">=14.0.0",
    "yarn": "1.x.x"
  },
  "license": "ISC",
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/index.js",
    "dev": "npm run clean && NODE_ENV=dev nodemon",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && etsc --config=esbuild.config.js && npm run apidoc",
    "test": "yarn truncateTestDatabase && yarn dbMigrateTest && NODE_ENV=test ts-mocha --require dotenv/config dotenv_config_path=.test.env --require ./src/index.ts 'src/tests/**/*.{js,ts}' -t 70000 --exit",
    "dbMigrateTest": "NODE_ENV=test node -r dotenv/config -e 'require(\"./postgrator/postgrator.js\").migrate()' dotenv_config_path=.test.env",
    "truncateTestDatabase": "NODE_ENV=test node -r dotenv/config -e 'require(\"./postgrator/postgrator.js\").truncateDatabase()' dotenv_config_path=.test.env",
    "apidoc": "apidoc -i \"./src/routes\" -o apidoc/"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-random-string": "^4.0.0",
    "debug": "^4.3.0",
    "express": "^4.17.1",
    "form-data": "^3.0.0",
    "getenv": "^1.0.0",
    "http-errors": "^1.8.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "luxon": "^1.27.0",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "postgrator": "^4.1.1",
    "rimraf": "^3.0.2",
    "sequelize": "^6.6.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/es6-promise": "^3.3.0",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "@types/sequelize": "^4.28.9",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.15.2",
    "apidoc": "^0.28.1",
    "chai": "^4.3.4",
    "codecov": "3.8.1",
    "dotenv": "^8.2.0",
    "esbuild-node-tsc": "^1.2.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-watch": "^7.0.0",
    "faker": "^5.5.3",
    "mocha": "^8.3.2",
    "nock": "^13.0.5",
    "nodemon": "^2.0.3",
    "nyc": "^15.0.1",
    "sinon": "^9.0.2",
    "supertest": "^6.1.3",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  },
  "directories": {
    "test": "test"
  },
  "apidoc": {
    "name": "Anywize",
    "description": "Anywize API",
    "sampleUrl": null
  }
}
